{
	"name":"swinde/contao4-downloadarchive",
	"description":"Define downloadarchives with permission for users to download single files.",
	"keywords":["download","archive","permission"],
	"type":"contao-bundle",
	"homepage":"http://internetservice.winde-ganzig.de/",
	"license":"LGPL-3.0-or-later",
	"authors":[
		{
			"name":"Felix Pfeiffer",
			"homepage":"http://www.felixpfeiffer.com/",
			"role":"Developer"
		},
		{
			"name":"Steffen Winde",
			"homepage":"http://internetservice.winde-ganzig.de/",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/swinde/contao4-downloadarchive/issues",
		"source":"https://github.com/swinde/contao4-downloadarchive"
	},
	"require": {
      "php": "^5.6|^7.1",
		"contao/core-bundle": "^4.4"
	},
	"require-dev": {
        	"contao/manager-plugin": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Swinde\\Downloadarchive\\": "src/"
		},
		"classmap": [
			"src/Resources/contao"
		],
		"exclude-from-classmap": [
			"src/Resources/contao/config",
			"src/Resources/contao/dca",
			"src/Resources/contao/languages",
			"src/Resources/contao/templates"
		]
	},
	"extra": {
		"contao-manager-plugin": "Swinde\\Downloadarchive\\ContaoManager\\Plugin"
	},
	"minimum-stability": "beta",
	"prefer-stable": true
}
